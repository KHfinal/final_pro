<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="member">
<resultMap type="map" id="memberMap"></resultMap>

	<!-- 
	<select id="selectOne" resultMap="memberMap" parameterType="string">
 		SELECT * FROM MEMBER WHERE USERID=#{userId} 	
 	</select> 
 	-->
	<select id="selectLogin" resultType="member" parameterType="string">
		SELECT * FROM MEMBER WHERE MEMBER_EMAIL =#{memberEemail}
	</select>
 	
 	<insert id="insertMember">
  	INSERT INTO MEMBER VALUES(#{memberEmail},#{memberPw},#{memberName},#{memberNickname},#{phone},default,null,default,default,null,null,null,default,null,null,null)
  	 <!-- VO과 맞추면된다. #{approvalStatus},#{approvalKey},#{reg_date},#{log_data},#{memberPFP},#{memberConcern},#{memberGender},#{memberBirthday},#{addr1},#{addr2},#{addr3}-->
  </insert>
</mapper>
