<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="schedule">
	<insert id="addSchedule" parameterType="schedule">
		insert into schedule values(#{userEmail},#{title},#{startDate},#{endDate},#{content})
	</insert>
	<resultMap type="map" id="event">
	</resultMap>
	<select id="loadEventList" parameterType="string" resultMap="event">
		select * from schedule where user_email=#{userEmail}
	</select>
</mapper>
